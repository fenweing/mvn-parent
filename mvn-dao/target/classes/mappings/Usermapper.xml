<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.mvn.dao.TestDao">
	<select id="getUserByName" resultType="org.mvn.common.User">
		select phone,name,nickName,sex from sys_user where
		name=#{name}
	</select>
</mapper>